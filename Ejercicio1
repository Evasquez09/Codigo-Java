public class Operacion {
	
	public static void main(String[] args) {
		
	int v1 = 10;
	int v2 = 5;	
	
	System.out.println("suma es igual a "+(v1+v2));
	
	System.out.println("resta es igual a "+(v1-v2));
	
	System.out.println("multiplicación es igual a "+(v1*v2));
	
	System.out.println("división es igual a "+(v1/v2));

	}
}
