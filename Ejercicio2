
public class Comparacion {

	public static void main(String[] args) {
		
		int v1 = 10;
		int v2 = 12;
		
		if (v1>=v2) {
			
			if(v1==v2){
				System.out.println("Las variable "+v1+" y "+v2+" son iguales");
			}else {
				System.out.println("La variable "+v1+" es mayor que "+v2);
			}
		}else {
			System.out.println("La variable "+v2+" es mayor que "+v1);
		}
	}
}
